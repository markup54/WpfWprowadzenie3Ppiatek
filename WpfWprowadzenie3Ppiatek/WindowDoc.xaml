<Window x:Class="WpfWprowadzenie3Ppiatek.WindowDoc"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWprowadzenie3Ppiatek"
        mc:Ignorable="d"
        Title="WindowDoc" Height="450" Width="800">
    <DockPanel>
        <!--
        ostatni element zajmuje wszystko co zostało
        -->
        <Button DockPanel.Dock="Bottom">1</Button>
        <Button DockPanel.Dock="Top">2</Button>
        <Button DockPanel.Dock="Right">3</Button>
        <Button>4</Button>
        <Button>5</Button>
        <Button DockPanel.Dock="Bottom">6</Button>
        <Button>7</Button>
        <Canvas>
            <Rectangle 
                Width="100"
                Height="200"
                Fill="DarkCyan"/>
        </Canvas>
    </DockPanel>
</Window>
